{
  "name": "playwrightjs",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "cucumber-js --config configs/cucumber-allure.mjs",
    "test:local": "cucumber-js --config configs/cucumber-local.mjs",
    "test:api": "cucumber-js --config configs/cucumber-api.mjs",
    "report": "allure serve allure-results",
     "test:pw": "playwright test --config=playwrightTestRunner/configs/playwright.config.mjs"
  },
  "engines": {
    "node": "^20.18.0",
    "npm": "10.8.2"
  },
  "author": "Min He",
  "license": "ISC",
  "description": "Playwright with/without Cucumber comparison",
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/register": "^7.25.9",
    "@cucumber/cucumber": "^11.2.0",
    "@faker-js/faker": "^9.6.0",
    "@playwright/test": "^1.50.1",
    "allure-commandline": "^2.32.2",
    "allure-cucumberjs": "^3.1.0",
    "allure-playwright": "^3.1.0",
    "axios": "^1.8.1",
    "csv-parser": "^3.2.0",
    "cucumber-playwright": "^1.2.1",
    "lodash": "^4.17.21",
    "nconf": "^0.12.1",
    "playwright": "^1.50.1"
  }
}
